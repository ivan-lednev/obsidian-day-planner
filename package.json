    "@tsconfig/svelte": "^5.0.4",
    "@types/chroma-js": "^2.4.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.7",
    "@types/luxon": "^3.6.2",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.5.5",
    "@types/unist": "^3.0.3",
